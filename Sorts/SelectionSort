#include <stdio.h>

int main() {
    int n, i, j, i_menor, temp;

    printf("Digite o tamanho do vetor: ");
    scanf("%d", &n);

    int v[n];

    printf("Digite os %d elementos do vetor:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &v[i]);
    }

    for (i = 0; i < n - 1; i++) {
        i_menor = i;
        for (j = i + 1; j < n; j++) {
            if (v[j] < v[i_menor]) {
                i_menor = j;
            }
        }
        if (i_menor != i) {
            temp = v[i];
            v[i] = v[i_menor];
            v[i_menor] = temp;
        }
    }

    printf("Vetor ordenado:\n");
    for (i = 0; i < n; i++) {
        printf("%d ", v[i]);
    }
    printf("\n");

    return 0;
}
